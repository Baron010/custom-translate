{
	"name": "custom-translate",
	"version": "2.2.3",
	"description": "A simple module for replacing words, letters, or patterns in a string with others.",
	"main": "index.js",
	"scripts": {
		"test": "npm run lint && jest",
		"lint": "eslint . --ext .js --ext .json"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/dragonfire535/custom-translate.git"
	},
	"keywords": [
		"translate",
		"translator"
	],
	"author": "dragonfire535",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/dragonfire535/custom-translate/issues"
	},
	"homepage": "https://github.com/dragonfire535/custom-translate#readme",
	"devDependencies": {
		"eslint": "^4.18.1",
		"eslint-config-aqua": "^2.0.1",
		"eslint-plugin-json": "^1.2.0",
		"jest": "^22.4.2"
	},
	"eslintConfig": {
		"extends": "aqua",
		"plugins": [
			"json"
		],
		"rules": {
			"camelcase": "off",
			"id-length": "off",
			"no-await-in-loop": "off",
			"no-console": "off",
			"no-process-env": "off",
			"prefer-const": "error"
		}
	}
}
